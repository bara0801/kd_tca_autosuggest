{
	"name": "kevinditscheid/kd-tca-autosuggest",
	"description": "This extension provides an autosuggest TCA configuration.",
	"keywords": [
		"typo3",
		"extension",
		"TCA"
	],
	"homepage": "https://github.com/bara0801/kd_tca_autosuggest",
	"support": {
		"issues": "https://github.com/bara0801/kd_tca_autosuggest/issues"
	},
	"authors": [
		{
			"name": "Kevin Ditscheid",
			"email": "kevinditscheid@gmail.com",
			"role": "Developer"
		}
	],
	"license": "GPL-3.0+",
	"type": "typo3-cms-extension",
	"require": {
		"typo3/cms-core": ">=7.6.0 <9.0.0"
	},
	"replace": {
		"kd_tca_autosuggest": "self.version",
		"typo3-ter/kd-tca-autosuggest": "self.version"
	},
	"autoload": {
		"psr-4": {
			"KevinDitscheid\\KdTcaAutosuggest\\": "Classes/"
		}
	}
}